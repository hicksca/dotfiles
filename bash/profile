if [ -z "${XDG_CONFIG_HOME}" ]; then
     	echo "I see the env var test passed"
	# export XDG_CONFIG_HOME=~/.config
fi

if [ -f $XDG_CONFIG_HOME/bash/bashrc ]; then
        source $XDG_CONFIG_HOME/bash/bashrc
fi
